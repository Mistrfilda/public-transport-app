{
	"name": "mistrfilda/public-transport-app",
	"description": "Nette: Standard Web Project",
	"keywords": [
		"nette"
	],
	"type": "project",
	"license": [
		"MIT",
		"BSD-3-Clause",
		"GPL-2.0",
		"GPL-3.0"
	],
	"require": {
		"php": ">= 7.3",
		"nette/application": "^3.0",
		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.0",
		"nette/di": "^3.0",
		"nette/finder": "^2.5",
		"nette/forms": "^3.0",
		"nette/http": "^3.0",
		"nette/mail": "^3.0",
		"nette/robot-loader": "^3.0",
		"nette/security": "^3.0",
		"nette/utils": "^3.0",
		"latte/latte": "^2.5",
		"tracy/tracy": "^2.6",
		"contributte/console": "^0.8",
		"nettrine/orm": "^0.5",
		"nettrine/dbal": "^0.6",
		"nettrine/migrations": "^0.6",
		"nettrine/cache": "^0.1",
		"nettrine/annotations": "^0.6",
		"mistrfilda/pid-api": "dev-master"
	},
	"require-dev": {
		"nette/tester": "^2.0",
		"phpstan/phpstan": "^0.12",
		"phpstan/phpstan-strict-rules": "^0.12",
		"phpstan/phpstan-nette": "^0.12",
		"phpstan/phpstan-doctrine": "^0.12",
		"jakub-onderka/php-parallel-lint": "^1.0",
		"tracy/tracy": "^2.6",
		"mockery/mockery": "^1.2",
		"symplify/easy-coding-standard-prefixed": "^7.2",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"classmap": [
			"src/Bootstrap.php"
		],
		"psr-4": {
			"App\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\": "src"
		}
	},
	"scripts": {
		"stan": "vendor/bin/phpstan analyse -l 7 -c phpstan.neon src tests --ansi",
		"lint": "vendor/bin/parallel-lint --colors src/ tests/",
		"cs": "vendor/bin/ecs -c ecs.yaml check src tests --ansi",
		"cs-fix": "vendor/bin/ecs -c ecs.yaml check src tests --fix --ansi",
		"test": "vendor/bin/tester tests/ -C",
		"test-coverage-html": "vendor/bin/tester tests/ -C --coverage coverage.html --coverage-src src/ ",
		"build-all": [
			"@lint",
			"@stan",
			"@cs"
		]
	},
	"minimum-stability": "stable"
}
